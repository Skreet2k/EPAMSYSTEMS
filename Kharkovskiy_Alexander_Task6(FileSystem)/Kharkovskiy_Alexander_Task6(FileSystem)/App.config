<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.diagnostics>
      <trace useGlobalLock="false" autoflush="true" indentsize="0">
        <listeners>
          <add name="ErrorTracer" type="System.Diagnostics.TextWriterTraceListener" initializeData="Error.log" traceOutputOptions="None" >
            <filter type="System.Diagnostics.EventTypeFilter"
      initializeData="Error" />
          </add>
          <add name="myListener2" type="System.Diagnostics.TextWriterTraceListener" initializeData="Trace.log">
            <filter type="System.Diagnostics.EventTypeFilter"
      initializeData="Information" />
          </add>
        </listeners>
      </trace>
    </system.diagnostics>
</configuration>